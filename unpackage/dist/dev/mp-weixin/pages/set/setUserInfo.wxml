<view class="container"><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}"><text class="cell-tit">{{i18n.telephone}}</text><text class="cell-tip">{{userInfo.personalPhone||''}}</text></view><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['/pages/set/setUserInfoName']]]]]}}" bindtap="__e"><text class="cell-tit">{{setMsg.name}}</text><text class="cell-tip">{{userInfo.name||setMsg.noSet}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" bindtap="__e"><text class="cell-tit">{{setMsg.avatar}}</text><image class="cell-tip portrait" src="{{userInfo.photoUrl||'/static/missing-face.png'}}"></image><text class="cell-more yticon icon-you"></text></view></view>